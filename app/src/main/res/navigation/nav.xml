<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.jk.spectr.ui.main.CountryListFragment"
        android:label="main_fragment"
        tools:layout="@layout/fragment_country_list" >
        <action
            android:id="@+id/action_mainFragment_to_memberFragment"
            app:destination="@id/memberFragment" />
    </fragment>
    <fragment
        android:id="@+id/memberFragment"
        android:name="com.jk.spectr.MemberFragment"
        android:label="fragment_member"
        tools:layout="@layout/fragment_member_list" />
</navigation>